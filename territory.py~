

class territory:
 
 IPCValue = 0
 installations=[]
 borders=[]
 territoryType='Land'
 originalOwner='UK'
 name='Territory'

 def __init__(self, territoryname, originalowner, value):
  self.name=territoryname
  self.originalOwner = originalowner
  self.IPCValue=value


def constructBorder(territory1, territory2):
 
 territory1.borders.append(territory2)
 territory2.borders.append(territory1)

